// c program to find a leap year
#include <stdio.h>

int main() {
int year;
printf("enter the year:");
scanf("%d", &year);

if(year % 400 == 0)
   printf("%d is a leap year",year);
else if(year % 100 == 0)
   printf("%d is not an leap year",year);
else if(year %4 == 0)
   printf("%d is a leap year",year);
else
printf("%d is not an leap year",year);

    return 0;
}
